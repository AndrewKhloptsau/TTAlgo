{
  "name": "AlgoServer-WebSpa",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "clean_nodes": "rimraf node_modules",
    "clean_dist": "rimraf --WebAdmin/wwwroot/dist",
    "fix_angular2_polyfill": "xcopy node_modules\\reflect-metadata node_modules\\angular2-universal-polyfills\\node_modules\\reflect-metadata /E/H/C/I/Y",
    "build_dev_vendor": "SET NODE_ENV=development&& webpack --config WebAdmin/webpack.config.vendor.js",
    "build_dev_main": "SET NODE_ENV=development&& webpack --config WebAdmin/webpack.config.js",
    "build_dev": "npm run fix_angular2_polyfill && npm run build_dev_vendor && npm run build_dev_main",
    "build_prod_vendor": "SET NODE_ENV=production&& webpack --mode production --color --config WebAdmin/webpack.config.vendor.js",
    "build_prod_main": "SET NODE_ENV=production&& webpack --mode production --color --config WebAdmin/webpack.config.js",
    "build_prod": "npm run fix_angular2_polyfill && npm run build_prod_vendor && npm run build_prod_main"
  },
  "dependencies": {
    "@angular/common": "~2.2.2",
    "@angular/compiler": "~2.2.2",
    "@angular/core": "~2.2.2",
    "@angular/forms": "~2.2.2",
    "@angular/http": "~2.2.2",
    "@angular/platform-browser": "~2.2.2",
    "@angular/platform-browser-dynamic": "~2.2.2",
    "@angular/router": "~3.2.2",
    "@angular/upgrade": "~2.2.2",
    "@aspnet/signalr": "1.0.26",
    "angular-in-memory-web-api": "^0.2.0",
    "angular2-jwt": "0.1.28",
    "angular2-masonry": "0.4.0",
    "aspnet-webpack": "^1.0.27",
    "bootstrap": "^3.3.7",
    "bootstrap-notify": "3.1.3",
    "es6-shim": "^0.35.3",
    "font-awesome": "4.7.0",
    "jquery": "3.1.1",
    "masonry-layout": "4.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^5.5.7",
    "string-format": "0.5.0",
    "tslib": "^1.10.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
