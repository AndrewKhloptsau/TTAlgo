{
  "name": "Angular2Spa",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "typings install",
    "typings": "typings",
    "clean_dist": "npm run rimraf --WebAdmin/wwwroot/dist",
    "build_dev_vendor": "SET NODE_ENV=development&& webpack --config WebAdmin/webpack.config.vendor.js",
    "build_dev_main": "SET NODE_ENV=development&& webpack --config WebAdmin/webpack.config.js",
    "build_dev": "npm run build_dev_vendor && npm run build_dev_main",
    "build_prod_vendor": "SET NODE_ENV=production&& webpack -p --color --config WebAdmin/webpack.config.vendor.js",
    "build_prod_main": "SET NODE_ENV=production&& webpack -p --color --config WebAdmin/webpack.config.js",
    "build_prod": "npm run build_prod_vendor && npm run build_prod_main"
  },
  "dependencies": {
    "@angular/common": "~2.2.2",
    "@angular/compiler": "~2.2.2",
    "@angular/core": "~2.2.2",
    "@angular/forms": "~2.2.2",
    "@angular/http": "~2.2.2",
    "@angular/platform-browser": "~2.2.2",
    "@angular/platform-browser-dynamic": "~2.2.2",
    "@angular/router": "~3.2.2",
    "@angular/upgrade": "~2.2.2",

    "angular-in-memory-web-api": "^0.2.0",
    "angular2-jwt": "0.1.28",
    "aspnet-webpack": "^1.0.27",
    "bootstrap": "^3.3.7",
    "bootstrap-notify": "3.1.3",
    "angular2-masonry": "0.4.0",
    "masonry-layout": "4.1.1",
    "es6-shim": "^0.35.3",
    "font-awesome": "4.7.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
