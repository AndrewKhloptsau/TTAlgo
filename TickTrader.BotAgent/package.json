    "css-loader": "^0.25.0",
    "css-to-string-loader": "0.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "expose-loader": "^2.0.0",
    "file-loader": "^0.9.0",
    "isomorphic-fetch": "^2.2.1",
    "mini-css-extract-plugin": "^1.3.9",
