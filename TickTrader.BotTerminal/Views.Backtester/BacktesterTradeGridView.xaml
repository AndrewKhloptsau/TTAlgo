                    xmlns:cal="http://www.caliburnproject.org"
                    xmlns:local="clr-namespace:TickTrader.BotTerminal">

    <ControlTemplate x:Key="BacktesterTradeGridView.Classic">
        <ContentControl cal:View.Model="{Binding GridView}"/>
